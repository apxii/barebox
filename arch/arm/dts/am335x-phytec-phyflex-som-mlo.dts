/dts-v1/;

#include "am33xx.dtsi"
#include "am335x-phytec-phyflex-som.dtsi"

/ {
	model = "Phytec phyFLEX AM335x";
	compatible = "phytec,phyflex-am335x-som", "phytec,am335x-som", "ti,am33xx";
};

/* Keep all bootsources disabled, we enable and register them
 * later while booting.
 */

&mmc1 {
	status = "disabled";
};

&gpmc {
	status = "disabled";
};
